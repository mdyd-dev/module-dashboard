---
name: google_analytics_form
resource: Customization
resource_owner: anyone
redirect_to: "/dashboard/google_analytics"
flash_notice: "You've successfuly set Google Analytics"
configuration:
  properties:
    tracking_id:
      validation: {}
default_payload: ''
---
{% form html-novalidate: false %}
  {% include 'modules/utils/utils' %}
  {% include form_errors %}
  {% include input with f.properties.tracking_id, type: "text", label: "Tracking ID" %}
  {% include submit_tag with "Save", class: "btn btn-primary" %}
{% endform %}
